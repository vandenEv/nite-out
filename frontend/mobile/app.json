{
    "expo": {
        "name": "Mobile",
        "slug": "Mobile",
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#090e0f"
        },
        "ios": {
            "supportsTablet": false
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#090e0f"
            }
        },
        "assetBundlePatterns": ["**/*"]
    }
}
